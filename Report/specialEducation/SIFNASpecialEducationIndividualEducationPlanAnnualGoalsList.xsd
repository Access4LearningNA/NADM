<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:include schemaLocation="../../Composite/SIFNAxPressCommonTypes.xsd" />
	<xs:include schemaLocation="../../Composite/SIFNAxPress.xsd" />
	<xs:complexType name="xSpecialEducationIndividualEducationPlanAnnualGoalListType">
		<xs:annotation>
			<xs:appinfo>
				<elementName> Annual Goals List </elementName>
				<sifChar>O</sifChar>
				<cedsId/>
				<cedsURL/>
			</xs:appinfo>
			<xs:documentation> List of goals to be sought for a special education student </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="annualGoal" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<elementName> Annual Goal </elementName>
						<sifChar>O</sifChar>
						<cedsId/>
						<cedsURL/>
					</xs:appinfo>
					<xs:documentation> Documentation of an annual goal to be achieved by a special education student
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="currentPerformace" type="xs:normalizedString" minOccurs="0" />
						<xs:element name="currentStatus" type="xs:normalizedString" minOccurs="0">
							<xs:annotation>
								<xs:appinfo>
									<elementName>
										Current Status
									</elementName>
									<sifChar>O</sifChar>
									<cedsId/>
									<cedsURL/>
								</xs:appinfo>
								<xs:documentation>
									Explanation of the student's current status relative to this goal.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="parentAcceptance" type="parentAcceptanceType" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:appinfo>
									<elementName>
										Parent Acceptance of Goal
									</elementName>
									<sifChar>O</sifChar>
									<cedsId/>
									<cedsURL/>
								</xs:appinfo>
								<xs:documentation>
									Parental agreement (or disagreement) with wworking toward this goal
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="goalType" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo>
									<elementName>
										Goal Type
									</elementName>
									<sifChar>O</sifChar>
									<cedsId/>
									<cedsURL/>
								</xs:appinfo>
								<xs:documentation>
									Category and reference information for goal
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="category" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<elementName>
													Category
												</elementName>
												<sifChar>O</sifChar>
												<cedsId/>
												<cedsURL/>
											</xs:appinfo>
											<xs:documentation>
												EXPLANATION
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:normalizedString">
												<xs:enumeration value="Academic" />
												<xs:enumeration value="Functional" />
												<xs:enumeration value="Transitional" />
												<!-- is this the right list? tqii -->
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="standardsReference">
										<xs:annotation>
											<xs:appinfo>
												<elementName>
													Standards Reference
												</elementName>
												<sifChar>O</sifChar>
												<cedsId/>
												<cedsURL/>
											</xs:appinfo>
											<xs:documentation>
												Reference for standard definition addressed by this goal
											</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="URI" type="xs:normalizedString" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Standards URI
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Location of the definition of the standard
														</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="identifier" type="xs:normalizedString" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Identifier
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Identifier or name of the standard
														</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="measurableObjectiveList" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo>
									<elementName>
										Measurable Objective List
									</elementName>
									<sifChar>O</sifChar>
									<cedsId/>
									<cedsURL/>
								</xs:appinfo>
								<xs:documentation>
									Objectives that will be achieved in the pursuit of this goal
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="measurableObjective">
										<xs:annotation>
											<xs:appinfo>
												<elementName>
													Measurable Objective
												</elementName>
												<sifChar>O</sifChar>
												<cedsId/>
												<cedsURL/>
											</xs:appinfo>
											<xs:documentation>
												Specific objective that will be achieved in the pursuit of this goal
											</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="description" type="xs:normalizedString" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Description
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Description of the practical appearance of the goal’s achievement
														</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="facilitatorList" minOccurs="1" maxOccurs="unbounded">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Facilitator List
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															List of people who will help the student achieve this goal
														</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="facilitator">
																<xs:annotation>
																	<xs:appinfo>
																		<elementName>
																			Facilitator
																		</elementName>
																		<sifChar>O</sifChar>
																		<cedsId/>
																		<cedsURL/>
																	</xs:appinfo>
																	<xs:documentation>
																		Person who will help the student achieve this goal
																	</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="identity" type="gStaffPersonType" minOccurs="0">
																			<xs:annotation>
																				<xs:appinfo>
																					<elementName>
																						Identity
																					</elementName>
																					<sifChar>O</sifChar>
																					<cedsId/>
																					<cedsURL/>
																				</xs:appinfo>
																				<xs:documentation>
																					Facilitator Identity
																				</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="contribution" type="xs:normalizedString" minOccurs="0">
																			<xs:annotation>
																				<xs:appinfo>
																					<elementName>
																						Contribution
																					</elementName>
																					<sifChar>O</sifChar>
																					<cedsId/>
																					<cedsURL/>
																				</xs:appinfo>
																				<xs:documentation>
																					The help this facilitor is asked to contribut to the achievement of this objective
																				</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="measurement" minOccurs="1" maxOccurs="unbounded">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Measurement
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Definition of the way we will know the student has achieved the goal
														</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="criterion">
																<xs:annotation>
																	<xs:appinfo>
																		<elementName>
																			Criterion
																		</elementName>
																		<sifChar>O</sifChar>
																		<cedsId/>
																		<cedsURL/>
																	</xs:appinfo>
																	<xs:documentation>
																		Criterion used to determine success or progress toward mastering this objective
																	</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="explanation" type="xs:normalizedString" minOccurs="0">
																			<xs:annotation>
																				<xs:appinfo>
																					<elementName>
																						Explanation
																					</elementName>
																					<sifChar>O</sifChar>
																					<cedsId/>
																					<cedsURL/>
																				</xs:appinfo>
																				<xs:documentation>
																					Descriptor of the the process of evaluation of success
																				</xs:documentation>
																			</xs:annotation>
																		</xs:element>
																		<xs:element name="metric" minOccurs="1" maxOccurs="unbounded">
																			<xs:annotation>
																				<xs:appinfo>
																					<elementName>
																						Metric
																					</elementName>
																					<sifChar>O</sifChar>
																					<cedsId/>
																					<cedsURL/>
																				</xs:appinfo>
																				<xs:documentation>
																					Definition of the quantitative measurement by which success will be judged. Three structures are defined.
																				</xs:documentation>
																			</xs:annotation>
																			<xs:complexType>
																				<xs:sequence>
																					<xs:element name="accuracyPercentEvaluation" type="xs:integer" minOccurs="0">
																						<xs:annotation>
																							<xs:appinfo>
																								<elementName>
																									Accuracy Percent
																								</elementName>
																								<sifChar>O</sifChar>
																								<cedsId/>
																								<cedsURL/>
																							</xs:appinfo>
																							<xs:documentation>
																								The percent of correct results that will be considered to represent success
																							</xs:documentation>
																						</xs:annotation>
																					</xs:element>
																					<xs:element name="trialCountEvaluation" minOccurs="1" maxOccurs="unbounded">
																						<xs:annotation>
																							<xs:appinfo>
																								<elementName>
																									Trial Count Specification
																								</elementName>
																								<sifChar>O</sifChar>
																								<cedsId/>
																								<cedsURL/>
																							</xs:appinfo>
																							<xs:documentation>
																								Specifies minimum of trials and success threshold
																							</xs:documentation>
																						</xs:annotation>
																						<xs:complexType>
																							<xs:sequence>
																								<xs:element name="successCount" type="xs:integer" minOccurs="0">
																									<xs:annotation>
																										<xs:appinfo>
																											<elementName>
																												Success Count
																											</elementName>
																											<sifChar>O</sifChar>
																											<cedsId/>
																											<cedsURL/>
																										</xs:appinfo>
																										<xs:documentation>
																											The percent of correct results that will be considered to represent success
																										</xs:documentation>
																									</xs:annotation>
																								</xs:element>
																								<xs:element name="attemptCount" type="xs:integer" minOccurs="0">
																									<xs:annotation>
																										<xs:appinfo>
																											<elementName>
																												Attempt Count
																											</elementName>
																											<sifChar>O</sifChar>
																											<cedsId/>
																											<cedsURL/>
																										</xs:appinfo>
																										<xs:documentation>
																											The number of attempts representing a completed trial
																										</xs:documentation>
																									</xs:annotation>
																								</xs:element>
																							</xs:sequence>
																						</xs:complexType>
																					</xs:element>
																					<xs:element name="otherEvaluation" type="xs:normalizedString" minOccurs="0">
																						<xs:annotation>
																							<xs:appinfo>
																								<elementName>
																									Other Evaluation Method
																								</elementName>
																								<sifChar>O</sifChar>
																								<cedsId/>
																								<cedsURL/>
																							</xs:appinfo>
																							<xs:documentation>
																								Description of other specific success measurement method
																							</xs:documentation>
																						</xs:annotation>
																					</xs:element>
																				</xs:sequence>
																			</xs:complexType>
																		</xs:element>
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="parentAcceptance" type="parentAcceptanceType" minOccurs="1" maxOccurs="1">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Parent Acceptance
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Parental agreement (or disagreement) with judging objective based on specified criterion
														</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="independentActionsList" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo>
									<elementName>
										Independent Actions List
									</elementName>
									<sifChar>O</sifChar>
									<cedsId/>
									<cedsURL/>
								</xs:appinfo>
								<xs:documentation>
									Activities or actions to be accomplished outside of a service context
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="independentAction" type="xs:normalizedString" minOccurs="0">
										<xs:annotation>
											<xs:appinfo>
												<elementName>
													Independent Action
												</elementName>
												<sifChar>O</sifChar>
												<cedsId/>
												<cedsURL/>
											</xs:appinfo>
											<xs:documentation>
												Activity or action to be accomplished outside of a service context
											</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="servicesList" minOccurs="1" maxOccurs="unbounded">
							<xs:annotation>
								<xs:appinfo>
									<elementName>
										Service List
									</elementName>
									<sifChar>O</sifChar>
									<cedsId/>
									<cedsURL/>
								</xs:appinfo>
								<xs:documentation>
									List of supporting resources required for student to achieve a goal
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="service">
										<xs:annotation>
											<xs:appinfo>
												<elementName>
													Service
												</elementName>
												<sifChar>O</sifChar>
												<cedsId/>
												<cedsURL/>
											</xs:appinfo>
											<xs:documentation>
												Support for student progress given by professional or para-professionals on a scheduled basis.
											</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="category" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Category
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Legal category of service
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:normalizedString">
															<xs:enumeration value="SpecialInstruction" />
															<xs:enumeration value="Speech" />
															<xs:enumeration value="Motor" />
															<xs:enumeration value="Counseling" />
															<xs:enumeration value="Vision/hearing" />
															<xs:enumeration value="Transportation" />
															<xs:enumeration value="InterpretingServices" />
															<xs:enumeration value="Orientation/mobility" />
															<xs:enumeration value="ParentTraining" />
															<!-- is this list correct? tqii -->
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="setting" minOccurs="0">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Setting
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Where service will be rendered
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:normalizedString">
															<xs:enumeration value="General" />
															<xs:enumeration value="Special" />
															<!-- how to add other? tqii -->
															<!-- is this list correct? tqii -->
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="serviceProvider" minOccurs="1" maxOccurs="unbounded">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																ServiceProvider
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Identity and role of person rendering support services
														</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="identity" type="gStaffPersonType" minOccurs="0">
																<xs:annotation>
																	<xs:appinfo>
																		<elementName>
																			Identity
																		</elementName>
																		<sifChar>O</sifChar>
																		<cedsId/>
																		<cedsURL/>
																	</xs:appinfo>
																	<xs:documentation>
																		Service Provider Identity
																	</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="role" minOccurs="0">
																<xs:annotation>
																	<xs:appinfo>
																		<elementName>
																			Role
																		</elementName>
																		<sifChar>O</sifChar>
																		<cedsId/>
																		<cedsURL/>
																	</xs:appinfo>
																	<xs:documentation>
																		Professional specialty applicable to the support of this goal.
																	</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:normalizedString">
																		<xs:enumeration value="Counselor" />
																		<xs:enumeration value="ReadingSpecialist" />
																		<!-- is this list correct? tqii -->
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
														</xs:sequence>
													</xs:complexType>
												</xs:element>
												<xs:element name="frequency" minOccurs="1" maxOccurs="unbounded">
													<xs:annotation>
														<xs:appinfo>
															<elementName>
																Frequency
															</elementName>
															<sifChar>O</sifChar>
															<cedsId/>
															<cedsURL/>
														</xs:appinfo>
														<xs:documentation>
															Prescription for amount of service to be given.
														</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="cycle" minOccurs="0">
																<xs:annotation>
																	<xs:appinfo>
																		<elementName>
																			Cycle
																		</elementName>
																		<sifChar>O</sifChar>
																		<cedsId/>
																		<cedsURL/>
																	</xs:appinfo>
																	<xs:documentation>
																		How often service must be rendered
																	</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:normalizedString">
																		<xs:enumeration value="5Day" />
																		<xs:enumeration value="6Day" />
																		<xs:enumeration value="10Day" />
																		<!-- How to add 'other'? tqii -->
																		<!-- is this list correct? tqii -->
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="duration" minOccurs="1" maxOccurs="unbounded">
																<xs:annotation>
																	<xs:appinfo>
																		<elementName>
																			Duration
																		</elementName>
																		<sifChar>O</sifChar>
																		<cedsId/>
																		<cedsURL/>
																	</xs:appinfo>
																	<xs:documentation>
																		Amount of time during cycle that student receives service
																	</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:sequence>
																		<xs:element name="directMinutes" type="xs:integer" minOccurs="0">
																			<xs:annotation>
																				<xs:appinfo>
																					<elementName>
																						Direct Minutes
																					</elementName>
																					<sifChar>O</sifChar>
																					<cedsId/>
																					<cedsURL/>
